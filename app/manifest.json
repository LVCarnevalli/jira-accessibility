{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "commands": {
    "option1": {
      "suggested_key": {
        "default": "Ctrl+1",
        "windows": "Alt+1",
        "linux": "Ctrl+1"
      },
      "description": "Option number 1"
    }
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage",
    "webNavigation",
    "webRequest",
    "*://*.google.com/",
    "tts",
    "commands"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "jira accessibility",
    "default_popup": "popup.html"
  }
}